html
    head
        title= title
    style
        include style.css
    body
        include user.pug
        h1= message
        img(class="header-image" src="https://scontent-frt3-2.xx.fbcdn.net/v/t1.15752-0/p280x280/96283981_706588866811520_4868712323302293504_n.jpg?_nc_cat=103&_nc_sid=b96e70&_nc_ohc=7y3uMLb1sR4AX-T3WYm&_nc_ht=scontent-frt3-2.xx&_nc_tp=6&oh=cc9c6815463a47589666130f969ba071&oe=5ED9111B")
        h1="Meme list"
        ul
            each meme in memes
                li(class="meme")
                   a(href="meme/" + meme.id class="meme-link") #{meme.name}
                   span(class="meme-price") #{meme.price}
                   img(src=meme.url class="meme-image")